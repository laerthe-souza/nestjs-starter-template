{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "[Debug] - Run Script: start:dev",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "args": [
        "start:dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "autoAttachChildProcesses": true,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "[Debug] - Run Script: test:dev",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "yarn",
      "args": [
        "test:dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "autoAttachChildProcesses": true,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}